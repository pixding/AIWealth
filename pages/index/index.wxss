.tip-area{
  color:#fff;
  line-height: 30px;
  font-size: 16px;
  text-align: center;
  margin-top:30px;
}

.btn-area{
  text-align: center;
  overflow: hidden;
}
.cal-btn{
  border:1px solid #fff;
  border-radius: 6px;
  color:#fff;
  display: inline-block;
  height: 40px;
  line-height: 40px;
  padding: 0 25px;
  background: #FD8238;
  font-size:15px;
}

.photo-area{
  text-align: center;
}

.photo-content{
  margin:0 auto;
  min-width: 240px;
  min-height: 180px;
  background: #f8f8f8;
  display: inline-block;
  border:8px solid #fff;
  border-radius: 8px;
  margin-top:20px;
  margin-bottom: 40px;
  box-shadow: 8px 8px 3px #333;
  background-image: url(data:image/jpeg;base64,);
  background-repeat: no-repeat;
  background-position: center;
  position: relative;
}
.photo-content.noshow{
  min-height: auto;
}

.photo-cancel{
  position: absolute;
  right:-12px;
  top:-12px;
  background:#fff;
  border:3px solid #fff;
  border-radius:100%;

}



